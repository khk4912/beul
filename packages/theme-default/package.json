{
  "name": "@beul-ssg/theme-default",
  "version": "0.1.0",
  "description": "Default theme for Beul SSG",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build && vite build -c vite.config.client.ts && tsc -p tsconfig.build.json --emitDeclarationOnly",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "exports": {
    ".": "./dist/index.js",
    "./client": "./dist/client.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "beul",
    "theme",
    "ssg",
    "react"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@beul-ssg/core": "workspace:*",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "neostandard": "^0.12.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1"
  }
}
